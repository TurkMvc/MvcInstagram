@using System.Web.Optimization
<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width">
    <link rel="icon" type="image/png" href="~/img/favicon.png">
    @Styles.Render("~/Css/PageStart")
    @Scripts.Render("~/Js/PageStart")
    @Html.Partial("_CumtomScripts")

</head>
<body>
    @RenderSection("RS_Menu", false)
    @RenderSection("RS_ProfilBar", false)
    @RenderSection("RS_PostItems", false)
    @RenderBody()
    @RenderSection("RS_Footer", false)
</body>
</html>